%header
  .conteiner
    %nav.navber-expand-lg.navber-white
      .navber-brand
        %a{:href => "/top"}
          %img.logo-mark{:alt => "ふぁしり", :src => "/assets/facili.png"}
        %a{:href => "/top"}
          %img.logo{:alt => "ふぁしり", :src => "/assets/facili_logo.png"}
      %button.naver-toggler{'data-toggle'=>'collapse', 'data-target'=>'#menu'}
        %span.navber-toggler-icon
      #menu.collapse.navbar-collapse
        -if user_signed_in?
          %ul.navbar-nav
            %li.nav-item= link_to'プロフィール編集', '/users/#{current_user.id}/edit', class: 'btn btn-success nav-link'
            %li.nav-item= link_to'ログアウト', destroy_user_session_path, method: :delete, class: 'btn btn-warning nav-link'
            %li.nav-item= link_to'このサイトの使い方', '/top/information', class: 'btn btn-primary nav-link'
          .sp-ransack-form.visible-xs-block.visible-sm-block
            = search_form_for(@q, :url=>{:controller=>"top", :action=>"search"}) do |f|
              = f.label :name_cont, '授業名'
              = f.search_field :name_cont
              = f.submit '検索'
        - else
          %ul.navbar-nav.mt-2
            %li.nav-item= link_to"新規登録",new_user_registration_path, class: "btn btn-success"
            %li.nav-item= link_to"ログイン",new_user_session_path, class: "btn btn-info"
            %li.nav-item= link_to'このサイトの使い方','/top/information', class: "btn btn-primary"
